SELECT
CO.CUSTOMER_ID
,
SUM(CASE
WHEN 
(
  (EXCLUSIONS<>'null' AND LENGTH(EXCLUSIONS)>0) OR (EXTRAS<>'null' AND EXTRAS IS NOT NULL AND LENGTH(EXTRAS)>0)
)=TRUE
THEN 1 ELSE 0
END)
AS CHANGES
,
SUM(CASE
WHEN 
(
  (EXCLUSIONS<>'null' AND LENGTH(EXCLUSIONS)>0) OR (EXTRAS<>'null' AND EXTRAS IS NOT NULL AND LENGTH(EXTRAS)>0)
)=TRUE
THEN 0 ELSE 1
END)
AS NO_CHANGES
FROM RUNNER_ORDERS AS RO
INNER JOIN CUSTOMER_ORDERS AS CO
ON CO.ORDER_ID = RO.ORDER_ID
WHERE PICKUP_TIME != 'null'
GROUP BY CO.CUSTOMER_ID